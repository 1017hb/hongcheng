@import '/app.wxss';
.goodList{
    display: flex;
    flex-wrap: wrap;
    margin: 0 20rpx;
}
.goodList-item{ 
  width: 30.5%;
  background-color: #fff;
  margin-top: 8rpx;
  border-radius: 10rpx;
  border: 1rpx solid #f4f4f4; 
}
.goodList-item:nth-of-type(3n+2){
  margin-left: 3%;
  margin-right: 3%;
}
.pictrue{
  width: 100%;
  height: 220rpx;
}
.pictrue image{
  border-radius: 10rpx 10rpx 0 0;
}
.goodList .goodList-item .underline .line1{line-height: 50rpx;}
.goodList .goodList-item .underline .money .num{font-weight: 800}
.goodList .goodList-item .underline{padding:10rpx 0 10rpx 20rpx;}
.goodList .goodList-item .underline .vip-money .num{font-size:22rpx;color:#aaa;font-weight:normal;}
.goodList .goodList-item .underline .vip-money{justify-content: space-between;width: 96%}
.goodList .goodList-item .underline .vip-money .iconfont{width:46rpx;height:44rpx;border-radius:50%;font-size:30rpx;}
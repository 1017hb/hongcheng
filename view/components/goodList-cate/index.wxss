@import '/app.wxss';
.goodList{
    display: flex;
    flex-wrap: wrap;
}
.goodList-item{ 
  width: 45%;
  background-color: #fff;
  margin: 8rpx 2%;
  border-radius: 10rpx;
  border: 1rpx solid #f4f4f4; 
}
.pictrue{
  width: 100%;
  height: 240rpx;
}
.pictrue image{
  border-radius: 10rpx 10rpx 0 0;
}
.goodList .goodList-item .underline .line1{line-height: 50rpx;}
.goodList .goodList-item .underline .money .num{font-weight: 800}
.goodList .goodList-item .underline{padding:10rpx 0 20rpx 20rpx;border-bottom:1px solid #f5f5f5;}
.goodList .goodList-item .underline .vip-money .num{font-size:22rpx;color:#aaa;font-weight:normal;}
.goodList .goodList-item .underline .vip-money{justify-content: space-between;width: 98%}
.goodList .goodList-item .underline .vip-money .iconfont{width:46rpx;height:44rpx;border-radius:50%;font-size:30rpx;}
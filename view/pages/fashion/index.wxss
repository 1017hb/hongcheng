.index .wrapper .goods-class{
  display: flex;
  flex-wrap: wrap;
  margin-top:16rpx;
  padding:0 26rpx 30rpx 26rpx; 
}
.index .wrapper .goods-class .item{
  width: 20%;
}
.index .wrapper .goods-class .item.none_vip{
  display:none;
}
.index .wrapper .goods-class .item .img-box{width:100%;height:130rpx;padding: 10%;border-radius: 50%;}
.index .wrapper .goods-class .item .img-box image{width:100%;height:100%;border-radius:50%;}
.index .wrapper .goods-class .item .pro-info{font-size:24rpx;color:#282828;text-align:center;height:42rpx;line-height:42rpx;border-bottom:0;border-top:0;padding:0 10rpx; margin-bottom: 20rpx;}


